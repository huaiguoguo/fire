{
    "name": "huaiguoguo/fire",
    "description": "Yii PHP Framework Version 2 - Development Package",
    "keywords": [
        "fire",
        "framework"
    ],
    "homepage": "no homepage",
    "type": "fire-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Guo Feng",
            "email": "gufe198@163.com",
            "homepage": "no homepage",
            "role": "Founder and project lead"
        }
    ],
    "support": {
        "issues": "https://github.com/huaiguoguo/fire/issues?state=open",
        "forum": "no homepage",
        "wiki": "no homepage",
        "source": "https://github.com/huaiguoguo/fire"
    },
    "minimum-stability": "dev",
    "replace": {
        "huaiguoguo/fire": "self.version"
    },
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*",
        "ezyang/htmlpurifier": "~4.6",
        "cebe/markdown": "~1.0.0 | ~1.1.0",
        "bower-asset/jquery": "2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
        "bower-asset/jquery.inputmask": "~3.2.2",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "~2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "cebe/indent": "*"
    },
    "suggest": {
        "yiisoft/yii2-coding-standards": "you can use this package to check for code style issues when contributing to yii"
    },
    "autoload": {
        "psr-4": {
            "fire\\": "framework/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}